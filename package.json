    "@angular/router": "~3.4.9",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "~2.5.35",
    "@types/jquery": "^2.0.34",
    "@types/node": "~6.0.45",
    "@types/slick-carousel": "^1.6.31",
    "autoprefixer": "^6.5.1",
    "calendar-utils": "0.0.36",
